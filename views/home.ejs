<div class="table-responsive">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>#</th>
        <th>Product Name</th>
        <th>Price(Per Day)</th>
        <th>Owner</th>
      </tr>
    </thead>
    <tbody>
      <% for (var i = 0; i < products.length ; i++) { %>
      
      <tr>
        <td><%= i+1 %></td>
        <td>
          <a href="/services/rent/<%= products[i].id %>"><%= products[i].name %></a>
        </td>
        <td><%= products[i].price %></td>
        <td><%= products[i].user.name %></td>
        <td><%= products[i]._id %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
</div>


<ul class="pagination">
  <% for(let i=0;i<pages;i++) { %>
    
  
        <% if(curr == i+1) { %>
          <li class="page-item active">
            <a href="/<%= i+1 %>" class="page-link"><%= i+1 %></a>
          </li>
        <% } else {%>
          <li class="page-item ">
            <a href="/<%= i+1 %>" class="page-link"><%= i+1 %></a>
          </li>
         <% } %>
    <% } %>

</ul>

